{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "rewrites": [
        {
            "source": "/push/onesignal/(.*)",
            "destination": "/push/onesignal/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/push/onesignal/(.*)",
            "headers": [
                {
                    "key": "Service-Worker-Allowed",
                    "value": "/push/onesignal"
                }
            ]
        }
    ]
}