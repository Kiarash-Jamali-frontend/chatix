{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "rewrites": [
        {
            "source": "/push/onesignal/:path*",
            "destination": "/push/onesignal/:path*",
            "has": [
                {
                    "type": "query",
                    "key": "appId",
                    "value": "**"
                },
                {
                    "type": "query",
                    "key": "sdkVersion",
                    "value": "**"
                }
            ]
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/push/onesignal/OneSignalSDKWorker.js",
            "headers": [
                {
                    "key": "Service-Worker-Allowed",
                    "value": "/"
                }
            ]
        }
    ]
}